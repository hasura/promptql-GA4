    "commander": "^14.0.0",
    "google-auth-library": "^10.1.0",
    "undici": "^7.13.0",
    "undici-types": "^7.13.0"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.4",
    "@types/node": "^20.11.30",
    "typescript": "^5.8.3"
